    "maplibre-gl": "^5.6.2",
    "rollup": "^4.47.1",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=16"
  },
  "homepage": "https://github.com/qziov/maplibre-reprojected#readme",
